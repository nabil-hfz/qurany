  "type": "commonjs",
  "scripts": {
    "build": "node_modules/.bin/rimraf ./dist && npx tsc",
    "dev": "nodemon --files --exec ts-node src/index.ts",
    "start": "tsc && node ./dist/src/index.js",
    "build:watch": "tsc --watch",
    "lint2": "eslint . --ext .ts",
    "test": "jest",
    "testWatch": "jest --coverage --watch",
    "testC": "jest --coverage",
    "docs": "typedoc",
    "typeorm:migrate": "typeorm migration:run -d dist/db/data-source-prod.js",
    "typeorm:revert": "npx typeorm-ts-node-esm migration:revert -d dist/db/data-source-prod.js",
    "typeorm:drop": "npx typeorm-ts-node-esm schema:drop -d dist/db/data-source-prod.js",
    "pm2": "tsc && pm2 start pm2.config.js"  },
  "keywords": [
